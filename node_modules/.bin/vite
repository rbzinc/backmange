#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/++/backmange/node_modules/.pnpm/vite@6.2.1_@types+node@22.13.10_jiti@2.4.2_sass@1.85.1_stylus@0.57.0_sugarss@5.0.0_postcss@8.5.3_/node_modules/vite/bin/node_modules:/mnt/d/++/backmange/node_modules/.pnpm/vite@6.2.1_@types+node@22.13.10_jiti@2.4.2_sass@1.85.1_stylus@0.57.0_sugarss@5.0.0_postcss@8.5.3_/node_modules/vite/node_modules:/mnt/d/++/backmange/node_modules/.pnpm/vite@6.2.1_@types+node@22.13.10_jiti@2.4.2_sass@1.85.1_stylus@0.57.0_sugarss@5.0.0_postcss@8.5.3_/node_modules:/mnt/d/++/backmange/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/++/backmange/node_modules/.pnpm/vite@6.2.1_@types+node@22.13.10_jiti@2.4.2_sass@1.85.1_stylus@0.57.0_sugarss@5.0.0_postcss@8.5.3_/node_modules/vite/bin/node_modules:/mnt/d/++/backmange/node_modules/.pnpm/vite@6.2.1_@types+node@22.13.10_jiti@2.4.2_sass@1.85.1_stylus@0.57.0_sugarss@5.0.0_postcss@8.5.3_/node_modules/vite/node_modules:/mnt/d/++/backmange/node_modules/.pnpm/vite@6.2.1_@types+node@22.13.10_jiti@2.4.2_sass@1.85.1_stylus@0.57.0_sugarss@5.0.0_postcss@8.5.3_/node_modules:/mnt/d/++/backmange/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
